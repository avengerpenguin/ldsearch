@prefix schema: <http://schema.org/> .
@prefix rnews: <http://iptc.org/std/rNews/2011-10-07#> .
@prefix og: <http://ogp.me/ns#> .
@prefix og2: <http://opengraphprotocol.org/schema/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix po: <http://purl.org/ontology/po/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix md: <http://www.w3.org/ns/md#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .


{ ?x rnews:headline      ?y } => { ?x schema:name         ?y } .
{ ?x og:title            ?y } => { ?x schema:name         ?y } .

{ ?x rnews:description   ?y } => { ?x schema:description  ?y } .
{ ?x dcterms:description ?y } => { ?x schema:description  ?y } .

{ ?x rnews:thumbnailUrl  ?y } => { ?x schema:thumbnailUrl ?y } .

{ ?x og:url              ?y } => { ?x schema:url          ?y } .
{ ?x og2:url             ?y } => { ?x schema:url          ?y } .

{ ?x md:item ?list . ?list rdf:first ?first . ?first schema:description ?y } => { ?x schema:description ?y } .
{ ?x md:item ?list . ?list rdf:first ?first . ?first schema:name ?y } => { ?x schema:name ?y } .
